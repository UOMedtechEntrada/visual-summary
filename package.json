        "react-dates": "^21.8.0",
        "react-dom": "^16.14.0",
        "react-redux": "^5.0.7",
        "react-select": "^2.4.4",
        "react-table": "^6.9.2",
        "react-tooltip": "^4.2.21",
        "react-with-direction": "^1.3.1",
        "recharts": "^2.0.9",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "webpack-cli": "^4.5.0",
