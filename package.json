    "description": "Elentra Module to integrate CBME Dashboard",
    "main": "build/sask-dashboard.js",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "visualization",
        "medicine",
        "react",
        "redux",
        "d3"
    ],
    "scripts": {
        "clean": "rimraf build/*",
        "build": "npm run clean && npm run build-code",
